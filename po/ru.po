msgstr "Нет соединения с сервером PulseAudio"

#: ../panel-plugin/pulseaudio-button.c:335
#, c-format
msgid "Volume %d%% (muted)"
msgstr "Громкость %d%% (откл.)"

#: ../panel-plugin/pulseaudio-button.c:337
#, c-format
msgid "Volume %d%%"
msgstr "Громкость %d%%"

