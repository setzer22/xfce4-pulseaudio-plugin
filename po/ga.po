#, c-format
msgid "Volume %d%c (muted)"
msgstr "Airde %d%c (gan fuaim)"

#: ../panel-plugin/pulseaudio-notify.c:203
#, c-format
msgid "Volume %d%c"
msgstr "Airde %d%c"

